{
  "name": "@stimuluz/gerd",
  "version": "0.1.0",
  "license": "MIT",
  "exports": {
    "./gerd": "./src/main.ts"
  },
  "tasks": {
    "gerd": "deno run -A src/main.ts",
    "up:gerd": "deno compile --output $HOME/.local/bin/gerd --include . --no-check -A src/main.ts",
    "un:gerd": "rm -f $HOME/.local/bin/gerd",
    "dev": "deno run -A --watch src/main.ts",
    "r:win": "deno compile --output dist/gerd-windows-x86_64.exe --target x86_64-pc-windows-msvc --include . --no-check -A src/main.ts",
    "r:macOld": "deno compile --output dist/gerd-macos-x86_64 --target x86_64-apple-darwin --include . --no-check -A src/main.ts",
    "r:macNew": "deno compile --output dist/gerd-macos-aarch64 --target aarch64-apple-darwin --include . --no-check -A src/main.ts",
    "r:linuxOld": "deno compile --output dist/gerd-linux-x86_64 --target x86_64-unknown-linux-gnu --include . --no-check -A src/main.ts",
    "r:linuxNew": "deno compile --output dist/gerd-linux-aarch64 --target aarch64-unknown-linux-gnu --include . --no-check -A src/main.ts",
    "release": "mkdir -p dist && deno task r:win && deno task r:macOld && deno task r:macNew && deno task r:linuxOld && deno task r:linuxNew",
    "test": "deno test",
    "new": "deno run -A scripts/new.ts"
  },
  "vendor": false,
  "importMap": "./import_map.json"
}
